{
  "name": "@pearnaly/semantic-version",
  "version": "1.0.5",
  "description": "Javascript/Typescript class to represent, parse and compare semantic versions according to semver.org",
  "repository": "https://github.com/pearnaly/semantic-version.git",
  "homepage": "https://github.com/pearnaly/semantic-version",
  "author": "Paul-Ã‰mile Arnaly <pearnaly@gmail.com>",
  "license": "MIT",
  "main": "./bundles/semVersion.js",
  "module": "./es5/index.js",
  "es2015": "./esm2015/index.js",
  "types": "./types/index.d.ts",
  "bugs": "https://github.com/pearnaly/semantic-version/issues",
  "files": [
    "bundles",
    "es5",
    "esm2015",
    "types"
  ],
  "keywords": [
    "Semantic Versioning"
  ],
  "scripts": {
    "clean": "rimraf types es5 esm2015 bundles",
    "buildTypes": "tsc --declaration --allowJs --emitDeclarationOnly --removeComments false --outDir types",
    "build": "npm run clean && npm run buildTypes && tsc && tsc -m es6 --outDir esm2015 && webpack",
    "watch": "npm run clean && tsc --watch",
    "test": "mocha -r ts-node/register src/**/*.test.ts",
    "lint": "eslint src",
    "publish": "npm publish --access=public"
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.17",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chai": "^4.3.4",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "ts-loader": "^9.1.1",
